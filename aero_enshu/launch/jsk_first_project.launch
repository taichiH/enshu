<launch>
  <!-- <arg name="speech_topic" default="/speech"/> -->
  <arg name="speech_topic" default="/windows/voice"/>
  <arg name="diff_min" default="0.30" />
  <node name="jsk_first_project" pkg="aero_enshu"
        type="jsk_first_project" output="screen">
    <remap from="/windows/voice" to="$(arg speech_topic)"/>
    <!-- <param name="0.09" value="($arg diff_min)" /> -->
    <!-- <param name="0.30" value="($arg diff_max)" /> -->
  </node>

  <node name="simulated_speak" pkg="negomo_enshu"
        type="simulated_speak.py" output="screen"/>

  <include file="$(find negomo_enshu)/examples/launch/plot.launch"/>

  <node name="example_interaction_cb" pkg="aero_enshu"
        type="interaction.py" output="screen">
    <remap from="/speech" to="$(arg speech_topic)" />
    <param name="time" value="1.0"/>
    <param name="phrase" value="なんですか"/>
    <param name="taskto" value="task2"/>
    <param name="phrase_refuse" value="I cannot interact at the moment."/>
    <param name="time_refuse" value="1.0"/>
  </node>

  <!--node name="negomo_sensor_hand" pkg="aero_enshu"
      type="detect_hand" output="screen"/-->

  <!-- <node name="negomo" pkg="negomo_enshu" -->
  <!--       type="negotiation_model"> -->
  <!--    <rosparam file="$(find negomo_enshu)/config/negomo.yaml" -->
  <!--              command="load"/> -->
  <!-- </node> -->
  <node name="rqt_logger_level" pkg="rqt_logger_level" type="rqt_logger_level" />
</launch>
